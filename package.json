{
  "name": "async-json-serializer",
  "version": "0.0.0",
  "description": "A library to stringify a JSON object across multiple async callbacks.",
  "main": "dist/async-json-serializer.js",
  "scripts": {
    "test": "mocha ./test/helpers/setup test/*",
    "tdd": "electron-mocha --renderer --interactive --recursive ./test/helpers/setup  test/",
    "prepublish": "npm run standard && npm run clean && npm run build",
    "standard": "standard",
    "build": "babel src --out-dir dist",
    "clean": "rm -rf dist",
    "ci": "npm run standard && npm run test"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "describe",
      "it",
      "assert",
      "beforeEach"
    ]
  },
  "keywords": [
    "json",
    "async",
    "stringify",
    "idle"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.0.0",
    "babel-eslint": "^4.0.5",
    "chai": "^2.0.0",
    "electron-mocha": "git://github.com/nathansobo/electron-mocha.git#interactive-option",
    "electron-prebuilt": "^0.30.1",
    "mocha": "^2.3.4",
    "random-seed": "^0.3.0",
    "standard": "^4.5.4"
  }
}
